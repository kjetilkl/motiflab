# Different supported gene identifier types
IDFormat	KEGG	KEGG	KEGG	http://www.genome.jp/dbget-bin/www_bget?ebr:{ID}
IDFormat	RGD ID	rgd	RGD	
IDFormat	MGI symbol	mgi_symbol	MGI	
IDFormat	RefSeq mRNA	refseq_mrna	RefSeq	http://www.ncbi.nlm.nih.gov/nuccore/{ID}
IDFormat	CCDS ID	ccds	CCDS	
IDFormat	NCBI locus tag	locus_tag	NCBI	
IDFormat	RefSeq DNA predicted	refseq_dna_predicted	RefSeq	http://www.ncbi.nlm.nih.gov/nuccore/{ID}
IDFormat	RefSeq DNA	refseq_dna	RefSeq	http://www.ncbi.nlm.nih.gov/nuccore/{ID}
IDFormat	EMBL	embl	EMBL	
IDFormat	TAIR symbol	tair_symbol	TAIR	
IDFormat	UniGene ID	unigene	UniGene	
IDFormat	IRGSP ID	irgsp	IRGSP	
IDFormat	TAIR locus	tair_locus	TAIR	
IDFormat	CGD gene	cgd	CGD	http://www.candidagenome.org/cgi-bin/locus.pl?locus={ID}
IDFormat	Ensembl Gene	ensembl_gene_id	Ensembl	[ENSG.+,http://www.ensembl.org/Homo_sapiens/Gene/Summary?db=core;g={ID}],[ENSMUSG.+,http://www.ensembl.org/Mus_musculus/Gene/Summary?db=core;g={ID}],http://www.ensembl.org/Multi/Search/Results?species=all;idx=;q={ID}
IDFormat	HGNC symbol	hgnc_symbol	HGNC	http://www.genenames.org/data/hgnc_data.php?match={ID}
IDFormat	Fantom ID	fantom	Fantom	
IDFormat	GenBank protein	protein_id	GenBank	
IDFormat	RefSeq mRNA predicted	refseq_mrna_predicted	RefSeq	http://www.ncbi.nlm.nih.gov/nuccore/{ID}
IDFormat	MGI ID	mgi_id	MGI	
IDFormat	Entrez Gene	entrezgene	Entrez	http://www.ncbi.nlm.nih.gov/gene/?term={ID}
IDFormat	RefSeq ncRNA predicted	refseq_ncrna_predicted	RefSeq	http://www.ncbi.nlm.nih.gov/nuccore/{ID}
IDFormat	WikiGene ID	wikigene_id	WikiGene	
IDFormat	UCSC ID	ucsc	UCSC	
IDFormat	WikiGene name	wikigene_name	WikiGene	
IDFormat	RGD symbol	rgd_symbol	RGD	
IDFormat	Ensembl Transcript	ensembl_transcript_id	Ensembl	http://www.ensembl.org/Multi/Search/Results?species=all;idx=;q={ID}
IDFormat	TAIR locus model	tair_locus_model	TAIR
IDFormat	SGD ID	sgd	SGD	


# Other identifier types (not necessarily gene IDs)
otherID	Ensembl Gene	[ENSG.+,http://www.ensembl.org/Homo_sapiens/Gene/Summary?db=core;g={ID}],[ENSMUSG.+,http://www.ensembl.org/Mus_musculus/Gene/Summary?db=core;g={ID}],http://www.ensembl.org/Multi/Search/Results?species=all;idx=;q={ID}
otherID	HGNC Symbol	http://www.genenames.org/data/hgnc_data.php?match={ID}
otherID	Jaspar Core	http://jaspar.genereg.net/cgi-bin/jaspar_db.pl?rm=present&collection=CORE&ID={ID}
otherID	MedLine	http://www.ncbi.nlm.nih.gov/pubmed/{ID}
otherID	PubMed	http://www.ncbi.nlm.nih.gov/pubmed/{ID}
otherID	RefSeq	[NP_.+,http://www.ncbi.nlm.nih.gov/protein/{ID}],http://www.ncbi.nlm.nih.gov/nuccore/{ID}
otherID	RegulonDB motif	http://regulondb.ccg.unam.mx/regulon?format=jsp&organism=&term={ID}
otherID	TF encyclopedia	http://www.cisreg.ca/cgi-bin/tfe/articles.pl?tfid={ID}
otherID	dbSNP	http://www.ncbi.nlm.nih.gov/projects/SNP/snp_ref.cgi?rs={ID}
otherID	RegulomeDB	http://www.regulomedb.org/snp/{ID}


